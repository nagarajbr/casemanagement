<fieldset>
  <section class="solidbody">
  <legend><%= set_subheader("User Local Office List") %></legend>

  <% if @all_user_local_office_list.present? %>

      <table>
        <caption style="text-align:inherit"> <b> User Local Office List <b><p></p><caption>
        <thead>
          <tr>
          <th> User ID </th>
          <th> User Name</th>
          <th> Local Office </th>

        </tr>
        </thead>
          <tbody>
          <% @all_user_local_office_list.each do |each_local_office| %>
            <tr>
                <td>  <%= each_local_office.user_id %></td>
                <td>  <%= each_local_office.user_name %></td>

                <td>  <%= drop_down_value_description(each_local_office.local_office_id) %></td>
                <td>
                   <%= link_to "Delete",user_local_offices_destroy_path(each_local_office.id), method: :delete, data: { confirm: 'Are you sure you want to delete? '},id: "hyperlink_alert" %>
                </td>
            </tr>
          <% end %>
        </tbody>
      </table>

  <%else%>
          <h5 class="subheader">No Records found.Click New to add Local Office(s) to user. </h5>

  <%end%>
   <p></p>
          <%= link_to 'New',user_local_offices_new_initialize_path, class: "button radius"%>

</section >
</fieldset>
