<ul>
	<div class="row">
		<div class="large-4 columns">
	        <% if arg_characteristic_type == 'work' &&  session["GENERAL_HEALTH_CHALNG_TO_WORK_CHAR_FOUND_ADD_FLAG"] == 'N' %>
	        	<p></p>
		        	<b>Characteristic: </b>
		        	<br>

		    	<p>
		        	<%= "#{drop_down_value_description(arg_client_characteristic.characteristic_id)}  work characteric" %>
		      </p>
	        <% else%>
	        	<li class="required">
		            <%= arg_object.label :characteristic_id, "Characteristic" %>
		            <%= arg_object.select :characteristic_id, arg_charcteristic_dropdown.collect { |p| ["#{p.short_description} ", p.id ] },{ :include_blank => 'select characteristic type' } %>
	        	</li>
	        <% end%>

	        <li class="required">
         		<%= arg_object.label :start_date %>
         	   	<%= arg_object.date_field :start_date, onkeyup:"validate_date_format(event)" %>
	        </li>

	        <li>

         		<%= arg_object.label :end_date %>
         	   	<%= arg_object.date_field :end_date, onkeyup:"validate_date_format(event)" %>
   	        </li>

	        <%= render "notes/notes_field_new_edit", object_argument: arg_notes %>
	    </div>
    </div>
</ul>