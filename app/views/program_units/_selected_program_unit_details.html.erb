<p>
	<table>
			<caption style="text-align:inherit"><b>Selected Program Unit </b><p></p></caption>
			  <thead>
			    <tr>
					<th> Program Unit ID </th>
					<th> Status </th>
					<th> Service Program </th>
					<th> Case Type </th>
					<th> Disposition </th>
					<th> Disposition Date </th>
					<th> Disposition Reason </th>
					<th> Processing Office </th>
					<th> Eligibility Determination Worker</th>
					<th> Case Manager</th>


				</tr>
			  </thead>
			    <tbody>
						<tr>
							<td>  <%= arg_selected_program_unit.id %></td>
							<td>  <%= drop_down_value_description(arg_selected_program_unit.program_unit_status) %>  </td>
							<td>  <%= get_service_program_name(arg_selected_program_unit.service_program_id) %></td>
							<td>  <%= drop_down_value_description(arg_selected_program_unit.case_type) %>  </td>



							<td>  <%= drop_down_value_description(arg_selected_program_unit.disposition_status) %>  </td>
							<td>  <%= format_db_date(arg_selected_program_unit.disposition_date)  %></td>
							<td>  <%= drop_down_value_description(arg_selected_program_unit.disposition_reason) %>  </td>
							<td>  <%= drop_down_value_description(arg_selected_program_unit.processing_location) %></td>
							<td>   <%= get_user_name(arg_selected_program_unit.eligibility_worker_id) if arg_selected_program_unit.eligibility_worker_id.present?%>  </td>
							<td>   <%= get_user_name(arg_selected_program_unit.case_manager_id) if arg_selected_program_unit.case_manager_id.present?%>  </td>

						</tr>
				</tbody>
			</table>
</p>