<fieldset>
  <section class="solidbody">
<%= set_subheader("Register a new school ") %>
<fieldset>
  <%if @client_managment == 'CLIENT_MANAGEMENT' %>
        <%= form_for(@school, url: create_school_from_client_management_path('CLIENT_MANAGEMENT'),:method => :post) do |f| %>
        <%= render "shared/common_error", object_argument:@school %>
        <ul>
          <div class="row">
            <div class="large-4 columns">
          <%= render 'form',arg_object: f,arg_notes:@notes  %>
            </div>
          </div>
        </ul>

        <p>
           <%= link_to 'Cancel', new_education_path('CLIENT'),class: "secondary button radius" %>
           <%= f.submit "Save", class: 'button radius' %>
        </p>

        <% end %>
  <%else%>
      <%= form_for(@school, url: create_school_path,:method => :post) do |f| %>
        <%= render "shared/common_error", object_argument:@school %>
        <ul>
          <div class="row">
            <div class="large-4 columns">
          <%= render 'form',arg_object: f,arg_notes:@notes  %>
            </div>
          </div>
        </ul>

        <p>
           <%= link_to 'Cancel', schools_search_path,class: "secondary button radius" %>
           <%= f.submit "Save", class: 'button radius' %>
        </p>

      <% end %>
  <%end%>

  </fieldset>
  </section>
</fieldset>