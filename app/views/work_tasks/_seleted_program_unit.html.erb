<p>
		<table>
			<caption style="text-align:inherit"><b>Selected Program Unit </b><p></p></caption>
			  <thead>
			    <tr>
					<th> Program Unit ID </th>
					<th> Status </th>
					<th> Service Program </th>
					<th> Case Type </th>
                    <th> Disposition </th>
					<th> Disposition Date </th>
					<th> Disposition Reason </th>
					<th> Processing Office </th>

				</tr>
			  </thead>
			    <tbody>
						<tr>
							<td>  <%= selected_program_unit.id %></td>
							<td>  <%= drop_down_value_description(selected_program_unit.program_unit_status) %>  </td>
							<td>  <%= get_service_program_name(selected_program_unit.service_program_id) %></td>

							<% if selected_program_unit.case_type.present? %>
							<td> <%= drop_down_value_description(selected_program_unit.case_type) %>  </td>

							<% end %>



							<td>  <%= drop_down_value_description(selected_program_unit.disposition_status) %>  </td>
							<td>  <%= format_db_date(selected_program_unit.disposition_date)  %></td>
							<td>  <%= drop_down_value_description(selected_program_unit.disposition_reason) %>  </td>
							<td>  <%= drop_down_value_description(selected_program_unit.processing_location) %></td>
						</tr>
				</tbody>
			</table>