<fieldset>
	 <section class="solidbody">
	 	<% if  @menu == 'HOUSEHOLD_MEMBER_STEP_WIZARD'%>
	 		<p>
				<%= (render 'households/selected_household_partial',arg_household: @household) if @household.present?%>
			</p>
	 		<%= form_for(@income, :url => update_household_member_unearned_income_path(@client.id,@income.id,'HOUSEHOLD_MEMBER_STEP_WIZARD'), method: :put) do |f| %>
		        <legend><%= set_subheader("Edit #{drop_down_value_description(@income.incometype)} income information for household member: #{client_full_name(@client)}") %> </legend>
			       <%= render "shared/common_error", object_argument: @income %><br>
			        <ul>
				     	<div class="row">
				          <div class="large-4 columns">
				               <%= render 'form',f: f,arg_income_types:@income_types,arg_notes:@notes %>
					 	  </div>
					    </div>
					</ul>
				    <p>
			      <%= link_to 'Cancel', start_household_member_registration_wizard_path, class: "secondary button radius" %>
			      <%= f.submit "Save", class: 'button radius'%>
			    </p>

			<% end %>

	 	<% else %>
	 		<%= form_for(@income, :url => update_unearned_income_path(@menu,@income.id), method: :put) do |f| %>
		        <legend><%= set_subheader("Edit #{drop_down_value_description(@income.incometype)} income information for: #{client_full_name(@client)}") %> </legend>
			       <%= render "shared/common_error", object_argument: @income %><br>
			        <ul>
				     	<div class="row">
				          <div class="large-4 columns">
				               <%= render 'form',f: f,arg_income_types:@income_types,arg_notes:@notes %>
					 	  </div>
					    </div>
					</ul>
				    <p>
			      <%= link_to 'Cancel', unearned_incomes_path(@menu), class: "secondary button radius" %>
			      <%= f.submit "Save", class: 'button radius'%>
			    </p>

			<% end %>
	 	<% end %>




 	</section>
</fieldset>