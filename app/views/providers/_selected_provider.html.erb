<table>
	<caption style="text-align:inherit"><b> Selected Provider </b><p></p></caption>
		  <thead>
		    <tr>
				<th> Provider Name </th>
				<th> City </th>
				<th> Office Type </th>
			</tr>
		  </thead>
		    <tbody>

					<tr>
						<td>  <%= @provider.provider_name %></td>
						<td>  <%= get_provider_city(@provider.id) %></td>
						<% if @provider.id == @provider.head_office_provider_id %>
							<td>  <%= "Main" %> </td>
						<% else %>
							<td>  <%= "Branch" %> </td>
						<% end %>
					</tr>
			</tbody>
		</table>