
		<legend><%= set_subheader("Search results") %></legend>
      	<table>
      		<caption style="text-align:inherit"> <b> Providers </b><p></p></caption>
			<thead>
				<tr>
					<th> Provider Name </th>
					<th> City </th>
					<th> Office Type </th>
					<th> Tax ID or SSN </th>
					<th> Status </th>
					<th> Action </th>
				</tr>
			</thead>
			<tbody>
				<% @providers.each do |provider| %>

				<tr>
					<td>  <%=  provider.provider_name %></td>
					<td>  <%=  get_provider_city(provider.id) %></td>
					<% if provider.id == provider.head_office_provider_id %>
						<td>  <%= "Main" %> </td>
					<% else %>
						<td>  <%= "Branch" %> </td>
					<% end %>
					<td>  <%=  provider.tax_id_ssn %></td>
					<td>  <%=   drop_down_value_description(provider.status) %></td>
					<td>
							<%= link_to "select",set_provider_in_session_path(provider),id: "hyperlink"%>
					</td>
				</tr>
				<% end %>
			</tbody>
		</table>
