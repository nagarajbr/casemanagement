<%= link_to "Exit Wizard", client_applications_path,class: 'secondary button' %>
<%= arg_object.submit "Back", :name => "back_button",class: 'secondary button' unless arg_instance.first_step? %>
<% if arg_instance.last_step? %>
	<%= arg_object.submit "Complete Application Intake",class: 'button' %>
	<!--<%= link_to "Save & Register",client_applications_path,method: :post,class: 'button radius'  %>-->


<% else %>
	<%= arg_object.submit "Next",:name => "next_button",class: 'button' %>
<% end %>

