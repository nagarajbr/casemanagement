				<br><br>
			</td>
			<th> <%= @income_date %> </th>
			<% if @pass == 'Fail' %>
				<td id = "col-result-fail">Fail</td>
			<%else%>
				<td id = "col-result-pass">Pass</td>
			<%end%>
			<td>

				<%#= link_to("Verify", wage_ui_match_path(@focus_client.id) , class: 'button radius primary')%>
				 <%= submit_tag "Review" ,:name => 'wage_button', class: "button radius" %>

			</td>
		</tr>
	</tbody>
</table>

