<fieldset>
<section class="solidbody">
<legend><%= set_subheader("Edit language information for provider:  #{@provider.provider_name}" ) %></legend>
<%= render "providers/selected_provider" %>
<%= form_for @providerlanguage, :url => update_provider_language_path(@providerlanguage.id), :method => :patch do |f| %>
  <%= render "shared/common_error", object_argument: @providerlanguage %>
	<%= render 'form',arg_object: f %>
 	<%= link_to 'Cancel', show_provider_language_path,class: "secondary button radius" %>
 	<%= f.submit "Save", class: 'button radius'%>
<% end %>
</section>
</fieldset>