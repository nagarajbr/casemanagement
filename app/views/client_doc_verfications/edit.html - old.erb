<%= form_for(@client_doc_verification,:url => update_client_doc_verfication_path(@client_application.id), method: :put) do |f| %>
  <%= render "shared/common_error", object_argument: @client_doc_verification %>
    <fieldset>
    	<legend><%= set_subheader("Document verification for selected Application") %></legend>
    	<ul>
	     	<div class="row">
	          <div class="large-6 columns">
	               <%= render 'form_document_verification',arg_object: f %>
		 	  </div>
		    </div>
		</ul>
	    <p>
	    	<%= link_to "Cancel",client_doc_verfications_path,class: 'secondary button radius'%>

	       <%= f.submit "Save",class: 'button radius'%>
	        </p>

    </fieldset>
<% end %>