<li class="required" >
  <%= arg_object.label :provider_service_id%>
  <%= arg_object.select :provider_service_id,ProviderService.item_list(@provider.id).collect { |p| ["#{drop_down_value_long_description(p.service_type)} ", p.id] },{ :include_blank => 'Select service type' } %>
</li>

<li class="required">
  <%= arg_object.label :agreement_start_date %>
  <%= arg_object.date_field :agreement_start_date, onkeyup:"validate_date_format(event)" %>
</li>

<li class="required">
  <%= arg_object.label :agreement_end_date %>
  <%= arg_object.date_field :agreement_end_date, onkeyup:"validate_date_format(event)" %>
</li>

