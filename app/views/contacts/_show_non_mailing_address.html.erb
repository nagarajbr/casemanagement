      <% if arg_addr == @address %>
        <legend style=" width: auto " class="legend_group">
          <b>Entered Physical Address:</b><br>
        </legend>
      <% else %>
        <legend style=" width: auto " class="legend_group">
          <b>Verified Physical Address:</b><br>
        </legend>
      <% end %>
      <% if arg_addr.address_line1.present? %>
        <% if arg_addr.non_mailing_in_care_of.present? %>
          <%= " C/O #{arg_addr.in_care_of}"%><br>
        <% end %>
        <%= arg_addr.non_mailing_address_line1 %><br>
        <% if arg_addr.non_mailing_address_line2.present? %>
          <%= arg_addr.non_mailing_address_line2 %><br>
        <% end %>
        <% if arg_addr.non_mailing_city.present? %>
        <%= " #{(arg_addr.non_mailing_city)}, #{drop_down_value_description(arg_addr.non_mailing_state)}"%>
        <% end %>
        <% if arg_addr.non_mailing_zip_suffix.present? %>
          <%= "#{arg_addr.non_mailing_zip}-#{arg_addr.non_mailing_zip_suffix}"%>
        <%else%>
          <%= "#{arg_addr.non_mailing_zip}"%>
        <% end %><br>
        <% if arg_addr.county.present? %>
          <%= drop_down_value_long_description(arg_addr.county) %><br>
        <% end %>
      <% else %>
        <%= arg_addr.errors.full_messages.last %>
      <% end %>
