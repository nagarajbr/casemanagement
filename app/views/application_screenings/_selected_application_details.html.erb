		<table>
			<caption style="text-align:inherit"><b>Selected Application </b><p></p>
		 	</caption>
			  <thead>
			    <tr>
					<th>  ID </th>
					<th> Primary Contact </th>
					<th> Application Date </th>

					<th> Intake Worker </th>
					<th>  Status </th>



				</tr>
			  </thead>
			    <tbody>
						<tr>
							<td>  <%= arg_selected_application.id %></td>
