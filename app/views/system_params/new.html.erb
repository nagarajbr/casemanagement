<fieldset>
<section class="solidbody">
<legend> <%= set_subheader("Enter a new system parameter item" ) %></legend>
   <%= form_for(@systemparams, :url => system_param_category_system_params_path, method: :post) do |f| %>
		<%= render 'form', arg_object: f%>
		<p>
      <%= link_to 'Back', system_param_category_path(@systemparamcategory.id),class: "secondary button radius" %>
