<fieldset>
<section class="solidbody">
<legend><%= set_subheader("Create a new code table item for: #{@codetable.name}" ) %></legend>
	<%= form_for(@codetableitem, :url => code_table_codetable_items_path(@codetable.id), method: :post) do |f| %>
		<%= render 'form', arg_object: f %>
		<p>
	      <%= link_to 'Back', code_table_path(@codetable),class: "secondary button radius" %>
	      <%= f.submit "Save" ,class: 'button radius' %>
	    </p>
	<%end%>
</section>
</fieldset>